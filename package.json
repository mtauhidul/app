    },
    "bugs": {
        "url": ""
    },
    "homepage": "",
    "port": "38125",
    "scripts": {
        "reinstall": "rm -r node_modules; rm package-lock.json; npm install",
        "sonar:local": "sonar-scanner -Dsonar.host.url=http://localhost:9000",
        "test": "npm run frontend:test",
        "coverage": "npm run frontend:coverage",
        "frontend:coverage": "npm run clean:tests:reports && NODE_ENV=production BUILD_ENV=test nyc --reporter=lcov --report-dir test_reports/coverage mocha --require @babel/register --require source-map-support/register --require ignore-styles --full-trace --bail **/__test__/**",
        "clean:tests:reports": "rm -rf test_reports",
        "all:install": "npm install && npm run frontend:install",
        "frontend:install": "cd frontend && npm install && cd ../..",
        "frontend:build:custom": "cd frontend && rm -rf ./build && webpack && cd ../..",
        "bs": "cd api && npm run dev",
        "fs": "npm run frontend:start:local",
        "fs:https": "HTTPS=1 npm run frontend:start:local",
        "frontend:start:local": "cd frontend && NODE_ENV=development BUILD_ENV=local webpack serve",
        "ft": "npm run frontend:test",
        "ftw": "npm run frontend:test:watch",
        "lint": "eslint --ext .jsx,.js frontend/ lib/",
        "lint:fix": "eslint --ext .jsx,.js frontend/ lib/ --fix"
    },
    "pre-push": [
        "coverage"
    ],
    "nyc": {
        "include": [],
        "exclude": [
            "coverage/*.js",
            "lib/tests.js",
            "node_modules",
            "postcss.config.js",
            "frontend/webpack.config.js",
            "frontend/build/www/*.js",
            "frontend/build/www/externals/*.js",
            "frontend/externals/*.js",
            "**/__test__",
            "__storage__"
        ],
        "extension": [
            ".js",
            ".jsx"
        ],
        "reporter": [
            "text-summary",
            "html",
            "clover",
            "json-summary"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": false
    },
